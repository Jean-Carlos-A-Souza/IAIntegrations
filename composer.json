{
  "name": "iafuture/iaf-backend",
  "type": "project",
  "description": "IAFuture (IAF) multi-tenant SaaS backend",
  "keywords": ["laravel", "saas", "multi-tenant", "openai"],
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "dedoc/scramble": "^0.13.10",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^11.0",
    "laravel/sanctum": "^4.0",
    "openai-php/laravel": "^0.10"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.15",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
